<html>
  <head>
    <title>Mockable Express - Add Endpoint</title>
  </head>
  <body>
    <h2>
      Add End Point
    </h2>
    <form method="POST" action="/add">
      <table>
        <tbody>
          <tr>
            <td>
              Path :
            </td>
            <td><%=domain%><input name="path" /></td>
          </tr>
          <tr>
            <td>
              Method :
            </td>
            <td>
              <div id="method">
                <select name="method">
                  <option value="get"> Get</option>
                  <option value="post">Post</option>
                  <option value="put">Put</option>
                  <option value="delete">Delete</option>
                </select>
              </div>
            </td>
          </tr>
          <tr>
            <td>
              Header :
            </td>
            <td>
              <textarea rows="4" cols="50" name="header">
{"Content-Type": "application/json"}</textarea
              >
            </td>
          </tr>
          <tr>
            <td>
              Body :
            </td>
            <td>
              <textarea rows="4" cols="50" name="body"> </textarea>
            </td>
          </tr>
          <tr>
            <td>
              <input type="submit" value="Submit" />
            </td>
          </tr>
        </tbody>
      </table>
    </form>
  </body>
</html>
