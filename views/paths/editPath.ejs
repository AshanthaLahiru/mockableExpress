<html>
  <head>
    <title>Mockable Express - Edit Endpoint</title>
  </head>
  <body>
    <h2>
      Edit End Point (Domain : <%=domainName%>)
    </h2>
    <form method="POST" action="/domain/paths/<%=domainId%>/<%=pathId%>/edit">
      <table>
        <tbody>
            <tr>
            <td>
              Name
            </td>
            <td>
              <input name="name" value="<%= name%>" />
            </td>
          </tr>
          <tr>
              <td>
                Description :
              </td>
              <td><input name="desc" value="<%= desc%>" /></td>
            </tr>
          <tr>
            <td>
              Path : <%= domainName%>
            </td>
            <td>
              <input name="path" value="<%= path%>" />
            </td>
          </tr>
          <tr>
            <td>
              Method :
            </td>
            <td>
              <div id="method">
                <select name="method">
                  <option value="get" <%=get%>>Get</option>
                  <option value="post" <%=post%>>Post</option>
                  <option value="put" <%=put%>>Put</option>
                  <option value="delete" <%=del%>>Delete</option>
                </select>
              </div>
            </td>
          </tr>
          <tr>
            <td>Status Code</td>
            <td><input name="statusCode" value=<%= statusCode%>></td>
          </tr>
          <tr>
            <td>
              Header :
            </td>
            <td>
              <textarea rows="4" cols="50" name="header">
<%=JSON.stringify(header)%></textarea
              >
            </td>
          </tr>
          <tr>
            <td>
              Body :
            </td>
            <td>
              <textarea rows="4" cols="50" name="body"><%=body%></textarea>
            </td>
          </tr>
          <tr>
            <td>
              <input type="submit" value="Submit" />
            </td>
          </tr>
        </tbody>
      </table>
    </form>
  </body>
</html>
